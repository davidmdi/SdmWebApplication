<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>webCode.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsersListServlet</servlet-name>
        <servlet-class>webCode.servlets.UsersListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadXmlServlet</servlet-name>
        <servlet-class>webCode.servlets.LoadXmlServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileUploadServlet</servlet-name>
        <servlet-class>webCode.servlets.FileUploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowAreasTableServlet</servlet-name>
        <servlet-class>webCode.servlets.ShowAreasTableServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccountServlet</servlet-name>
        <servlet-class>webCode.servlets.AccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ZoneSelectedServlet</servlet-name>
        <servlet-class>webCode.servlets.ZoneSelectedServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AreaInfoServlet</servlet-name>
        <servlet-class>webCode.servlets.AreaInfoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SingleAreaMenuServlet</servlet-name>
        <servlet-class>webCode.servlets.SingleAreaMenuServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StoreOrdersServlet</servlet-name>
        <servlet-class>webCode.servlets.storeOwnerOptions.StoreOrdersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ZoneNameServlet</servlet-name>
        <servlet-class>webCode.servlets.generalServlets.ZoneNameServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserNameServlet</servlet-name>
        <servlet-class>webCode.servlets.generalServlets.UserNameServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ZoneStoresListServlet</servlet-name>
        <servlet-class>webCode.servlets.generalServlets.ZoneStoresListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OpenNewStoreServlet</servlet-name>
        <servlet-class>webCode.servlets.storeOwnerOptions.OpenNewStoreServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddNewStoreServlet</servlet-name>
        <servlet-class>webCode.servlets.storeOwnerOptions.AddNewStoreServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/pages/loginPage/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsersListServlet</servlet-name>
        <url-pattern>/userslist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadXmlServlet</servlet-name>
        <url-pattern>/loadXml</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileUploadServlet</servlet-name>
        <url-pattern>/uploadXmlFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowAreasTableServlet</servlet-name>
        <url-pattern>/areasTable</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccountServlet</servlet-name>
        <url-pattern>/accountTable</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ZoneSelectedServlet</servlet-name>
        <url-pattern>/zoneSelected</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AreaInfoServlet</servlet-name>
        <url-pattern>/areaInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SingleAreaMenuServlet</servlet-name>
        <url-pattern>/singleAreaMenu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StoreOrdersServlet</servlet-name>
        <url-pattern>/storeOrdersPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ZoneNameServlet</servlet-name>
        <url-pattern>/zoneName</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserNameServlet</servlet-name>
        <url-pattern>/userName</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ZoneStoresListServlet</servlet-name>
        <url-pattern>/zoneStoresList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OpenNewStoreServlet</servlet-name>
        <url-pattern>/newStorePage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddNewStoreServlet</servlet-name>
        <url-pattern>/addNewStore</url-pattern>
    </servlet-mapping>
</web-app>

